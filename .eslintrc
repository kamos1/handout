{
	"parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "es6": true,
        "jasmine": true,
        "jest": true,
        "jquery": true,
        "mocha": true,
    },
	"extends": "eslint:recommended",
    "rules": {
        "no-undef": 0,
        "no-unused-labels": 0,
        "no-unused-vars": 0
    }
}